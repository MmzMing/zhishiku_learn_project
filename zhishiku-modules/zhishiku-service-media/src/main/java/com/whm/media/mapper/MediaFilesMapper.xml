<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.whm.media.mapper.MediaFilesMapper">
    <sql id="selectMediaFilesVo">
        SELECT
            id,organization_id,organization_code,organization_name,file_name,file_type,tags,bucket,file_path,file_id,url,user_name,create_date,change_date,status,remark,audit_status,audit_mind,file_size,create_by,create_name,create_time,update_by,update_name,update_time,deleted
        FROM media_files
