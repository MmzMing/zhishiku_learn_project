<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.whm.media.mapper.MediaProcessMapper">
    <sql id="selectMediaProcessVo">
        SELECT
            id,file_id,file_name,bucket,file_path,status,create_date,finish_date,fail_count,url,error_msg,create_by,create_name,create_time,update_by,update_name,update_time,deleted
        FROM media_process
    </sql>
    <select id="pageQuery" parameterType="com.whm.media.domain.po.MediaProcess" resultType="com.whm.media.domain.po.MediaProcess">
        <include refid="selectMediaProcessVo"/>
        where 1=1
        <if test="param.id != null and param.id != ''">
            and id = #{param.id}
        </if>
        <if test="param.fileId != null and param.fileId != ''">
            and file_id = #{param.fileId}
        </if>
        <if test="param.fileName != null and param.fileName != ''">
            and file_name = #{param.fileName}
        </if>
        <if test="param.bucket != null and param.bucket != ''">
            and bucket = #{param.bucket}
        </if>
        <if test="param.filePath != null and param.filePath != ''">
            and file_path = #{param.filePath}
        </if>
        <if test="param.status != null and param.status != ''">
            and status = #{param.status}
        </if>
        <if test="param.createDate != null">
            and create_date = #{param.createDate}
        </if>
        <if test="param.finishDate != null">
            and finish_date = #{param.finishDate}
        </if>
        <if test="param.failCount != null and param.failCount != ''">
            and fail_count = #{param.failCount}
        </if>
        <if test="param.url != null and param.url != ''">
            and url = #{param.url}
        </if>
        <if test="param.errorMsg != null and param.errorMsg != ''">
            and error_msg = #{param.errorMsg}
        </if>
        <if test="param.createBy != null and param.createBy != ''">
            and create_by = #{param.createBy}
        </if>
        <if test="param.createName != null and param.createName != ''">
            and create_name = #{param.createName}
        </if>
        <if test="param.createTime != null">
            and create_time = #{param.createTime}
        </if>
        <if test="param.updateBy != null and param.updateBy != ''">
            and update_by = #{param.updateBy}
        </if>
        <if test="param.updateName != null and param.updateName != ''">
            and update_name = #{param.updateName}
        </if>
        <if test="param.updateTime != null">
            and update_time = #{param.updateTime}
        </if>
        <if test="param.deleted != null and param.deleted != ''">
            and deleted = #{param.deleted}
        </if>
    </select>
</mapper>